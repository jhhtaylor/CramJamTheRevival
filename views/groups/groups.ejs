<%- include ('../partials/header') %> 
<h1>Groups Page</h1>

<% groups.forEach(group => { %>
    <div>
        <h4><%= group.name %></h4>
        <% for (var i = 0; i < group.members.length; i++) { %>
            <li> <%= group.members[i] %> </li>
        <% } %>
    </div>
<% }); %>
<%- include ('../partials/footer') %> 
